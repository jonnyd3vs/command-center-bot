      "Read(./server/data/**)",
      "Read(./server/lib/**)",
      "Read(./errors/**)",
      "Read(./**/*.cache)",
      "Read(./**/*.jar)",
      "Read(./**/*.png)",
      "Read(./**/target/**)",
      "Read(./**/build/**)"
